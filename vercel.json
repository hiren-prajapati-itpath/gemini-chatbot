{
  "version": 2,
  "name": "gemini-caching-chatbot",
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "src/index.ts": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/src/index.ts"
    },
    {
      "source": "/(.*)",
      "destination": "/src/index.ts"
    }
  ]
}
